{
  "name": "rojak",
  "version": "0.0.0",
  "description": "A starter project including full setup for Jekyll, GulpJS, SASS, BrowserSync, Browserify & more",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ajmal Afif",
  "license": "ISC",
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^1.3.7",
    "browserify-shim": "^3.8.10",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "1.0.0",
    "gulp-cloudflare": "^0.1.3",
    "gulp-filter": "^3.0.1",
    "gulp-flatten": "^0.2.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-gh-pages-cname": "^0.5.2",
    "gulp-install": "^0.6.0",
    "gulp-main-bower-files": "^1.2.2",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-uglify": "^1.4.1",
    "install": "^0.1.8",
    "main-bower-files": "^2.9.0",
    "npm": "^3.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajmalafif/jekyll-gulp-sass-browser-sync.git"
  },
  "keywords": [
    "jekyll",
    "gulp",
    "sass",
    "browsersync",
    "google page insights",
    "browserify",
    "github pages"
  ],
  "bugs": {
    "url": "https://github.com/ajmalafif/jekyll-gulp-sass-browser-sync/issues"
  },
  "homepage": "https://github.com/ajmalafif/jekyll-gulp-sass-browser-sync",
  "dependencies": {
    "browserify": "^11.2.0",
    "critical": "^0.6.0",
    "glob": "^5.0.15",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^2.1.1",
    "gulp-plumber": "^1.0.1",
    "vinyl-source-stream": "^1.1.0"
  }
}
